{
  "extends": "@vanyauhalin/eslint-config-typescript",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "promise/prefer-await-to-callbacks": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "__suite__",
          "__test__"
        ]
      }
    ]
  }
}
